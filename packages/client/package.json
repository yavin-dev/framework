{
  "name": "@yavin/client",
  "version": "0.4.0",
  "description": "A node and web client for query fetching",
  "keywords": [
    "yavin",
    "elide",
    "fili"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yavin-dev/framework.git#master"
  },
  "license": "MIT",
  "author": "yavin@yahooinc.com",
  "homepage": "https://github.com/yavin-dev/framework/tree/master/packages/client#readme",
  "bugs": {
    "url": "https://github.com/yavin-dev/framework/issues"
  },
  "sideEffects": false,
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    "./*": {
      "import": "./lib/esm/*.js",
      "require": "./lib/cjs/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/types/*"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean:cjs": "shx rm -rf lib/cjs",
    "clean:esm": "shx rm -rf lib/esm",
    "clean:types": "shx rm -rf lib/types",
    "clean": "npm-run-all --silent -p clean:*",
    "babel:esm": "babel --quiet --config-file ./.babelrc-esm.cjs src --out-dir lib/esm --extensions '.ts'",
    "babel:cjs": "babel --quiet --config-file ./.babelrc-cjs.cjs src --out-dir lib/cjs --extensions '.ts'",
    "build:js:esm": "npm run clean:esm && node set-package-type.js lib/esm module && npm run babel:esm --",
    "build:js:cjs": "npm run clean:cjs && node set-package-type.js lib/cjs commonjs && npm run babel:cjs --",
    "build:js:types": "tsc -p tsconfig-types.json",
    "build:js": "npm-run-all --silent -p build:js:*",
    "build": "npm run clean && npm run build:js",
    "watch": "npm-run-all -l --parallel 'build:js:* -- --watch'",
    "prepare": "npm run build",
    "lint:ts": "tsc --noEmit",
    "lint": "npm-run-all --silent -c lint:*",
    "build:test:web": "npm run build:js:esm && webpack --config test/webpack.config.js",
    "build:test:node": "npm run build:js:cjs && shx rm -rf test/dist/build && tsc -p test/tsconfig.json && node set-package-type test/dist/build commonjs",
    "test:web": "testem ci --file testem.cjs",
    "test:node": "npm run build:test:node && qunit --require ./test/dist/build/test/setup.js ./test/dist/build/test/tests/**/*",
    "test": "npm-run-all --aggregate-output --silent -n -p -c test:*"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/lodash": "^4.14.182",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.4.2",
    "@types/qunit": "^2.11.3",
    "babel-plugin-import-graphql": "^2.8.1",
    "graphql": "^16.5.0",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.19.1",
    "shx": "^0.3.4",
    "testem": "^3.6.0",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
