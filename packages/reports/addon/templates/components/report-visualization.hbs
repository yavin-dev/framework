<div class="report-visualization" ...attributes>
  {{#let (component this.visualizationComponent) as |VisualizationComponent|}}
    <VisualizationComponent
      @model={{ember-array this.visualizationHash @annotationData}}
      @options={{@report.visualization.metadata}}
      @onUpdateReport={{@onUpdateReport}}
      @containerComponent={{or @container this}}
      @isEditing={{@isEditing}}
    />
  {{/let}}
</div>