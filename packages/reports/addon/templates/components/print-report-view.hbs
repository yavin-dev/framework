<div class="print-report-view__visualization-main">
  {{#if this.hasNoData}}
    <div class="print-report-view__visualization-no-results">
      No results available.
    </div>
  {{else}}
    {{#let @report.visualization.manifest.visualizationComponent as |VisualizationComponent|}}
      <VisualizationComponent
        class="print-report-view__visualization"
        @request={{@report.request}}
        @response={{@response}}
        @settings={{@report.visualization.metadata}}
        {{!-- @onUpdateReport={{noop}} --}}
        {{!-- @onUpdateSettings={{noop}} --}}
        @isEditing={{false}}
        @isReadOnly={{true}}
        @isPrint={{true}}
        @manifest={{@report.visualization.manifest}}
      />
    {{/let}}
  {{/if}}
</div>
