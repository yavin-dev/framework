{{#if @value.isFavorite}}
  <DenaliIcon
    class="link p-x-30"
    @icon="star-solid"
    @size="small"
    {{on "click" (fn this.toggleFavorite @value.isFavorite)}}
  />
{{else}}
  <DenaliIcon
    class="link p-x-30"
    @icon="star"
    @size="small"
    {{on "click" (fn this.toggleFavorite @value.isFavorite)}}
  />
{{/if}}