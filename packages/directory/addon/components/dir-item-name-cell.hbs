<LinkTo @route={{this.itemLink}} @model={{this.itemId}} class="dir-item-name-cell is-black">
  <span>{{@value.title}}</span>
</LinkTo>

{{#if @value.tempId}}
  <DenaliChip
    @size="small"
    @backgroundColor="grey"
    @backgroundShade="300"
    @textColor="grey"
    @textShade="700"
    class="dir-item-name-cell__unsaved-label"
  >
    UNSAVED
  </DenaliChip>
{{/if}}