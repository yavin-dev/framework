{{#if this.occlusion}}
  {{#component this.collectionRenderer
    items=@tableData
    estimateHeight=@estimateHeight
    containerSelector=@containerSelector
    bufferSize=@bufferSize
    buffer=@bufferSize
    cell-layout=(mixed-height-layout @rowDimensions)
    as |row idx|
  }}
    {{yield row idx}}
  {{/component}}
{{else}}
  {{#each @tableData as |row idx|}}
    {{yield row idx}}
  {{/each}}
{{/if}}