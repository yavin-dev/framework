<div class="pie-chart-config" ...attributes>
  {{#if (not (eq @options.series.type "dimension"))}}
    <div class="navi-visualization-config__warn-msg">No configuration options available.</div>
  {{/if}}
  <NaviVisualizationConfig::SeriesChart
    @request={{@request}}
    @response={{@response}}
    @seriesConfig={{readonly @options.series.config}}
    @seriesType={{readonly @options.series.type}}
    @onUpdateConfig={{this.onUpdateSeriesConfig}}
  />
</div>
