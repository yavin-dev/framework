<div id={{this.guid}} class="navi-visualization" ...attributes>
  <this.visualizationComponent
    @model={{ember-array this.visualizationHash @annotationData}}
    @options={{@report.visualization.metadata}}
    @onUpdateReport={{@onUpdateReport}}
    @containerComponent={{or @container this.getContainer}}
    @isEditing={{@isEditing}}
  />
</div>