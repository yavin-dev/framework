{{#if (not (is-empty this.columnValue))}}
  <div class="table-cell-content {{dasherize @column.fragment.type}} table-cell-url" ...attributes>
    <DenaliLink
      href={{this.columnValue}}
      target="_blank"
      rel="noopener noreferrer"
      title={{this.columnValue}}
    >
      {{this.displayValue}}
    </DenaliLink>
  </div>
{{else}}
  <NaviCellRenderers::Dimension
    @data={{@data}}
    @column={{@column}}
    @request={{@request}}
    @isRollup={{@isRollup}}
    @isGrandTotal={{@isGrandTotal}}
    ...attributes
  />
{{/if}}