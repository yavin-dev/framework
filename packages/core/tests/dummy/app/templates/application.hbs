{{page-title "Core"}}

<DenaliNavbar @isResponsive={{true}} as |Nav|>
  <Nav.Left as |Left|>
    <Left.Item>Navi Core</Left.Item>
  </Nav.Left>
  <Nav.Center as |Center|>
    <Center.LinkTo @route="bar-chart"><DenaliIcon @icon="chart-bar-vertical" /> Bar Chart</Center.LinkTo>
    <Center.LinkTo @route="goal-gauge"><DenaliIcon @icon="dashboard" /> Goal Gauge</Center.LinkTo>
    <Center.LinkTo @route="line-chart"><DenaliIcon @icon="chart-line" /> Line Chart</Center.LinkTo>
    <Center.LinkTo @route="pie-chart"><DenaliIcon @icon="chart-pie" />Pie Chart</Center.LinkTo>
    <Center.LinkTo @route="metric-label"><DenaliIcon @icon="layout-one" /> Metric Label</Center.LinkTo>
    <Center.LinkTo @route="table"><DenaliIcon @icon="table" /> Table</Center.LinkTo>
    <Center.LinkTo @route="buttons"><DenaliIcon @icon="cursor-arrow" /> Buttons</Center.LinkTo>
    <Center.LinkTo @route="date-pick"><DenaliIcon @icon="cursor-arrow" /> Date Pick</Center.LinkTo>
  </Nav.Center>
  <Nav.Right as |Right|>
    <Right.Item><DataAvailability @position="right" @dataSources={{array "bardOne"}} /></Right.Item>
  </Nav.Right>
</DenaliNavbar>

<div class="outlet">{{outlet}}</div>
