{
  "name": "navi-app",
  "version": "0.3.0",
  "description": "Template project for your Navi application",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/yavin/tree/master/packages/app"
  },
  "license": "MIT",
  "author": "team-navi@googlegroups.com",
  "directories": {
    "test": "tests"
  },
  "main": "index.js",
  "scripts": {
    "build": "cmd=\"ember build --environment=$npm_config_environment\" && echo $cmd && eval $cmd ",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "startElideUI": "cross-env APP_ENV=localElide npm run start",
    "startElideWS": "cd ../webservice/ && cross-env APP_ENV=localElide ./gradlew execJar",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "deploy-demo-app": "export BUILD_NAVI_DEMO=true; ember github-pages:commit --message 'update gh-pages' --destination ../../; echo 'gh-pages updated, now run:\ngit push origin gh-pages:gh-pages'"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "ember-auto-import": "^1.10.1",
    "express": "^4.14.0",
    "merge": "~1.2.0"
  },
  "devDependencies": {
    "@denali-design/ember": "^1.0.0-alpha.29",
    "@ember/jquery": "^0.6.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "@html-next/vertical-collection": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "cross-env": "^7.0.3",
    "ember-auto-import": "^1.5.3",
    "ember-basic-dropdown": "^3.0.19",
    "ember-cli": "~3.25.3",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "^1.1.3",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-mirage": "~2.2.0",
    "ember-cli-sass-less": "0.0.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-update": "^0.34.10",
    "ember-data": "~3.24.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.0.4",
    "ember-font-awesome": "^3.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-raf-scheduler": "^0.1.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.1",
    "ember-template-lint": "^2.18.1",
    "ember-test-waiters": "^1.1.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-qunit": "^4.0.0",
    "loader.js": "^4.7.0",
    "navi-core": "0.3.0",
    "navi-dashboards": "0.3.0",
    "navi-data": "0.3.0",
    "navi-directory": "0.3.0",
