<div class="dashboard-filters-collapsed-filter__dimension">
  {{filter.dimension.longName}}
</div>

<div class="dashboard-filters-collapsed-filter__operator">
  {{filterOperator}}
</div>

<div class="dashboard-filters-collapsed-filter__values">
  {{#each filterValues as |value idx|}}{{unless (eq idx 0) ", "}}<div class="dashboard-filters-collapsed-filter__value">{{value}}</div>{{/each}}
</div>