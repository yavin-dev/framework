<NaviVisC3Chart
  @classNames={{this.classNames}}
  @data={{this.config.data}}
  @dataSelection={{this.config.dataSelection}}
  @axis={{this.config.axis}}
  @grid={{this.config.grid}}
  @legend={{this.config.legend}}
  @point={{this.config.point}}
  @tooltip={{this.config.tooltip}}
  @containerComponent={{@containerComponent}}
  @metrics={{map-by "displayName" this.request.metricColumns}}
  ...attributes
/>
